<h1>kicks</h1>
<div class="shoes-wrapper row">
  <% @shoes.each do |shoe| %>
    <div class="shoe">
      <div class="shoe-head">
        <div class="name">
        </div>
      </div>
      <div class="image center-block">
        <%= link_to (image_tag shoe.image.url(:medium), class:'img-responsive'), shoe_path(shoe) %>
      </div>
      <p class="notes">
        <%= shoe.notes %>
      </p>
    </div>
  <% end %>
</div>
